<h2>Dashboard</h2>
    
  <p> Hello, {{ user.firstname }}. </p>

{{#if places}}
<a href="/{{user.firstname}}/memory">Go to your memories</a>
<br></br>
<a href="#" id="zoomOut">Your places:</a>
<ul>
      {{#each places}}
      <li >
        <a href="#" onclick="zoomIn({{this.location}})">{{this.name}}</a> |
        <a href="/{{this._id}}/edit">Edit</a> |
        <a href="/{{this._id}}/delete">Delete</a>
      </li>
      {{/each }}
    </ul>
{{/if}}

<a href="/new">Add a new place</a>
<br></br>
<a href="/logout"> Log Out </a>
<br></br>

<div id="map"></div>

<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
